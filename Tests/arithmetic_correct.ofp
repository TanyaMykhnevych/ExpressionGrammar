float area() {
	 float length = 4.5;
	 float width = 8.0;
	 float area = length*width;
	 print("Area of Rectangle is:");
	 print(area);

	 return area;
}

int fact(int n) {
       int output;
       if(n==1){
         return 1;
       }
       # Recursion
       output = fact(n-1)* n;
       return output;
}
